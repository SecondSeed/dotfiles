{
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "s",
                "w"
            ],
            "commands": [
                "editor.action.surroundWithSnippet"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "R"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "h"
            ],
            "commands": [
                "references-view.showCallHierarchy"
            ]
        },
        {
            "before": [
                "g",
                "s"
            ],
            "commands": [
                "editor.showSupertypes"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "e"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "p",
                "e"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "<C-o>"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "before": [
                "<Leader>",
                "t",
                "z"
            ],
            "commands": [
                "workbench.action.toggleZenMode"
            ]
        },
        {
            "before": [
                "<Leader>",
                "a",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<Leader>",
                "t",
                "l"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<Leader>",
                "c",
                "t"
            ],
            "commands": [
                "workbench.action.closePanel"
            ]
        },
        {
            "before": [
                "<Leader>",
                "c",
                "o"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "g",
                "H"
            ],
            "commands": [
                "workbench.action.moveActiveEditorGroupLeft"
            ]
        },
        {
            "before": [
                "g",
                "L"
            ],
            "commands": [
                "workbench.action.moveActiveEditorGroupRight"
            ]
        },
        {
            "before": [
                "g",
                "K"
            ],
            "commands": [
                "workbench.action.moveActiveEditorGroupUp"
            ]
        },
        {
            "before": [
                "g",
                "J"
            ],
            "commands": [
                "workbench.action.moveActiveEditorGroupDown"
            ]
        },
        {
            "before": [
                "<Leader>",
                "s",
                "i"
            ],
            "commands": [
                "workbench.files.action.showActiveFileInExplorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "w"
            ],
            "commands": [
                "workbench.action.switchWindow"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.new"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "p"
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "c"
            ],
            "commands": [
                "editor.debug.action.runToCursor"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                "workbench.action.debug.stepInto"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "workbench.action.debug.stepOver"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "workbench.action.debug.continue"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "workbench.action.debug.stepOut"
            ]
        },
        {
            "before": [
                "Q"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "i"
            ],
            "commands": [
                "revealFileInOS"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "i"
            ],
            "commands": [
                "editor.action.organizeImports"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "m"
            ],
            "commands": [
                "editor.action.showContextMenu"
            ]
        },
        {
            "before": [
                "<Leader>",
                "a",
                "p"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        } 
    ],
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-s>": false,
        "<C-q>": false
    },
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "C:\\config\\.vscodevimrc",
    "files.autoSave": "afterDelay",
    "window.openFoldersInNewWindow": "on",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.iconTheme": null,
    "vsicons.dontShowNewVersionMessage": true,
    "vim.foldfix": true,
    "vim.leader": " ",
    "vim.scroll": 5,
    "vim.autoSwitchInputMethod.enable": true,
    "vim.autoSwitchInputMethod.defaultIM": "1033",
    "vim.autoSwitchInputMethod.obtainIMCmd": "D:\\im-select\\im-select.exe",
    "vim.autoSwitchInputMethod.switchIMCmd": "D:\\im-select\\im-select.exe {im}",
    "vim.argumentObjectClosingDelimiters": [
        ")",
        "]",
        ">",
        "}"
    ],
    "vim.argumentObjectOpeningDelimiters": [
        "(",
        "[",
        "<",
        "{"
    ],
    "vim.highlightedyank.enable": true,
    "vim.replaceWithRegister": true,
    "vim.smartRelativeLine": true,
    "vim.visualstar": true,
    "files.associations": {
        ".vscodevimrc": "vimrc"
    },
    "vim.showMarksInGutter": true,
    "vim.statusBarColorControl": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "redhat.telemetry.enabled": true,
    "workbench.colorTheme": "Monokai",
    "go.toolsManagement.autoUpdate": true,
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path.windows": "C:\\Users\\xxmhn\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.preferredDarkColorTheme": "Monokai Dimmed",
    "[python]": {
        "editor.formatOnType": true
    },
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "C_Cpp.intelliSenseEngine": "disabled"
}